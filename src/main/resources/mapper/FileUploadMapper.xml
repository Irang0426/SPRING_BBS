<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bbs.demo.mapper.FileUploadMapper">
    <insert id="insertFile" parameterType="Files">
        INSERT INTO files (note_id, file_name, files)
        VALUES (#{noteId}, #{fileName}, #{files})
    </insert>
</mapper>